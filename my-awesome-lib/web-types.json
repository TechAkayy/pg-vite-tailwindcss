{
  "$schema": "http://json.schemastore.org/web-types",
  "name": "my-awesome-lib",
  "framework": "tailwindcss",
  "version": "3.8.11",
  "contributions": {
    "html": {
      "elements": [
        {
          "name": "my-icon",
          "attributes": [
            {
              "name": "name",
              "description": "Note, this is not a real component in this project, added here just to show an example of how it is defined in web-types.",
              "customType": "icon",
              "placeholder": "line-md:iconify1"
            },
            {
              "name": "width",
              "customType": "select",
              "default": "24px",
              "options": [
                "12px",
                "16px",
                "20px",
                "24px",
                "28px",
                "32px",
                "36px"
              ]
            },
            {
              "name": "height",
              "customType": "select",
              "default": "24px",
              "options": [
                "12px",
                "16px",
                "20px",
                "24px",
                "28px",
                "32px",
                "36px"
              ]
            }
          ]
        }
      ],
      "attributes": [
        {
          "name": "color",
          "customType": "select",
          "options": []
        }
      ]
    }
  }
}
